<table class="table table-bordered table-striped data-table">
    <thead>
        <tr>
            <th>Description</th>
            <th>Quantity</th>
            <th>Expiration Date</th>
            <th>Bin</th>
            <th>Bin Location</th>
        </tr>
    </thead>
    <tbody>
        {{#each this}}
        <tr data-id="{{id}}">
            <td>{{description}}</td>
            <td>{{quantity}}</td>
            <td>{{expirationDate}}</td>
            <td>{{bin.binLabel}}</td>
            <td>{{bin.binLocation.locationName}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>